{
  "version": 1,
  "index_patterns": [
    "*"
  ],
  "settings": {
    "number_of_shards": 1,
    "number_of_replicas": 0
  },
  "mappings": {
    "dynamic_templates": [
      {
        "string_as_keywords": {
          "match": "string",
          "mapping": {
            "type": "keyword"
          }
        }
      },
      {
        "indexed_longs": {
          "match": "long",
          "mapping": {
            "type": "long",
            "index": true
          }
        }
      },
      {
        "indexed_doubles": {
          "match": "double",
          "mapping": {
            "type": "double",
            "index": true
          }
        }
      }
    ],
    "properties": {
      "@timestamp": {
        "type": "date",
        "format": "epoch_millis"
      }
    }
  }
}