{
  "version": 2,
  "index_patterns": [
    "domainevents-*"
  ],
  "aliases": {
    "domainevents": {}
  },
    "mappings": {
      "dynamic_templates": [
        {
          "on_as_dates": {
            "match": "*_on",
            "mapping": {
              "type": "date",
              "format": "epoch_millis"
            }
          }
        }
      ],
      "properties": {
        "data": {
          "type": "object"
        }
      }
    }
}
