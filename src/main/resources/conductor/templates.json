{
  "data": [
    {
      "id": "log4j_sysout",
      "type": "grouptemplate",
      "attributes": {
        "name": "Log4j HTTP requests to Sysout",
        "sources": {
          "pattern": "rel=='logevents'",
          "many": true,
          "parameters": {"category": "jetty"}
        },
        "consumers": {
          "pattern": "type=='soutlogging'"
        }
      }
    },
    {
      "id": "metrics_sysout",
      "type": "grouptemplate",
      "attributes": {
        "name": "Log metrics to Sysout",
        "sources": {
          "pattern": "rel=='metricevents'",
          "many": true
        },
        "consumers": {
          "pattern": "type=='soutmetrics'"
        }
      }
    },
    {
      "id": "metrics_jmx",
      "type": "grouptemplate",
      "attributes": {
        "name": "Expose metrics to JMX for VisualVM integration",
        "sources": {
          "pattern": "rel=='metricevents'",
          "many": true
        },
        "consumers": {
          "pattern": "type=='jmxmetrics'"
        }
      }
    },
    {
      "id": "greeter_neo4jdomainevents",
      "type": "grouptemplate",
      "attributes": {
        "name": "Project Greeter events into Neo4j",
        "sources": {
          "pattern": "rel=='domainevents'"
        },
        "consumers": {
          "pattern": "type=='neo4jdomainevents'",
          "parameters": {
            "database": "neo4j"
          }
        }
      }
    }
  ]
}