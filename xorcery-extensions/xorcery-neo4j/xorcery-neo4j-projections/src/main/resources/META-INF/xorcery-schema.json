{
  "$schema" : "https://json-schema.org/draft/2020-12/schema",
  "type" : "object",
  "$id" : "http://xorcery.exoreaction.com/modules/com.exoreaction.xorcery/xorcery-neo4j-projections/schema",
  "title" : "xorcery-neo4j-projections configuration JSON Schema",
  "properties" : {
    "neo4jprojections" : {
      "$ref" : "#/$defs/neo4jprojections"
    }
  },
  "$defs" : {
    "neo4jprojections" : {
      "type" : "object",
      "properties" : {
        "enabled" : {
          "type" : "string",
          "default" : "{{ neo4jdatabase.enabled }}",
          "description" : "Default: {{ neo4jdatabase.enabled }} ({{ neo4jdatabase.enabled }})"
        },
        "eventBatchSize" : {
          "type" : [ "number", "string" ],
          "default" : 1024,
          "description" : "Default: 1024"
        },
        "maxThreadCount" : {
          "type" : [ "number", "string" ],
          "default" : -1,
          "description" : "Default: -1"
        }
      }
    }
  }
}