{
  "$schema" : "https://json-schema.org/draft/2020-12/schema",
  "type" : "object",
  "$id" : "http://xorcery.exoreaction.com/modules/com.exoreaction.xorcery/xorcery-neo4j-browser/schema",
  "title" : "xorcery-neo4j-browser configuration JSON Schema",
  "properties" : {
    "neo4jbrowser" : {
      "$ref" : "#/$defs/neo4jbrowser"
    }
  },
  "$defs" : {
    "neo4jbrowser" : {
      "type" : "object",
      "properties" : {
        "enabled" : {
          "type" : "string",
          "default" : "{{ neo4jdatabase.enabled }}",
          "description" : "Default: {{ neo4jdatabase.enabled }} ({{ neo4jdatabase.enabled }})"
        },
        "ssl" : {
          "type" : "object",
          "properties" : {
            "enabled" : {
              "type" : "string",
              "default" : "{{ neo4jbrowser.enabled ? certificates.enabled | false }}",
              "description" : "Default: {{ neo4jbrowser.enabled ? certificates.enabled | false }} ({{ neo4jbrowser.enabled ? certificates.enabled | false }})"
            }
          }
        }
      }
    }
  }
}