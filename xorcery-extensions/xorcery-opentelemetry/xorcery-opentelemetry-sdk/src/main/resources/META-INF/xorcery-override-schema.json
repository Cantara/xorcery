{
  "$id" : "http://xorcery.exoreaction.com/applications/com.exoreaction.xorcery/xorcery-opentelemetry-sdk/override-schema",
  "title" : "xorcery-opentelemetry-sdk configuration override JSON Schema",
  "$schema" : "https://json-schema.org/draft/2020-12/schema",
  "type" : "object",
  "properties" : {
    "instance" : {
      "$ref" : "#/$defs/instance"
    },
    "application" : {
      "$ref" : "#/$defs/application"
    },
    "system" : {
      "$ref" : "#/$defs/system"
    },
    "defaults" : {
      "$ref" : "#/$defs/defaults"
    },
    "hk2" : {
      "$ref" : "#/$defs/hk2"
    },
    "DefaultRunLevelContext" : {
      "$ref" : "#/$defs/DefaultRunLevelContext"
    },
    "configuration" : {
      "$ref" : "#/$defs/configuration"
    },
    "log4j2" : {
      "$ref" : "#/$defs/log4j2"
    },
    "opentelemetry" : {
      "$ref" : "#/$defs/opentelemetry"
    },
    "secrets" : {
      "$ref" : "#/$defs/secrets"
    }
  },
  "$defs" : {
    "instance" : {
      "type" : "object",
      "additionalProperties" : false,
      "properties" : {
        "id" : {
          "type" : "string",
          "default" : "{{ instance.host}}.{{ application.name}}",
          "description" : "Default: {{ instance.host}}.{{ application.name}} (.application)"
        },
        "host" : {
          "type" : "string",
          "default" : "{{ CALCULATED.hostName }}",
          "description" : "Default: {{ CALCULATED.hostName }} ({{ CALCULATED.hostName }})"
        },
        "fqdn" : {
          "type" : "string",
          "default" : "{{ instance.host }}.{{ instance.domain }}",
          "description" : "Default: {{ instance.host }}.{{ instance.domain }} (.xorcery.test)"
        },
        "ip" : {
          "default" : "{{ CALCULATED.ip | null }}",
          "description" : "Default: null"
        },
        "domain" : {
          "type" : "string",
          "default" : "xorcery.test",
          "description" : "Default: xorcery.test"
        },
        "environment" : {
          "type" : "string",
          "default" : "development",
          "description" : "Default: development"
        },
        "tags" : {
          "type" : [ "array", "string" ]
        },
        "home" : {
          "type" : "string",
          "default" : "{{ SYSTEM.user_dir }}",
          "description" : "Default: {{ SYSTEM.user_dir }} ({{ SYSTEM.user_dir }})"
        },
        "resources" : {
          "type" : "string",
          "default" : "{{ instance.home }}",
          "description" : "Default: {{ instance.home }} ({{ instance.home }})"
        },
        "uri" : {
          "type" : "string",
          "default" : "http://{{ instance.fqdn }}/",
          "description" : "Default: http://{{ instance.fqdn }}/ (http://.xorcery.test/)"
        },
        "api" : {
          "type" : "string",
          "default" : "{{ instance.uri }}api/",
          "description" : "Default: {{ instance.uri }}api/ (http://.xorcery.test/api/)"
        },
        "locale" : {
          "default" : null,
          "description" : "Default: null"
        }
      }
    },
    "application" : {
      "type" : "object",
      "additionalProperties" : false,
      "properties" : {
        "name" : {
          "type" : "string",
          "default" : "application",
          "description" : "Default: application"
        },
        "version" : {
          "type" : "string",
          "default" : "unknown",
          "description" : "Default: unknown"
        }
      }
    },
    "system" : {
      "type" : "object",
      "additionalProperties" : true,
      "properties" : { }
    },
    "defaults" : {
      "type" : "object",
      "additionalProperties" : false,
      "properties" : {
        "enabled" : {
          "type" : [ "boolean", "string" ],
          "default" : true,
          "description" : "Default: true"
        },
        "connectTimeout" : {
          "type" : "string",
          "default" : "5s",
          "description" : "Default: 5s"
        },
        "idleTimeout" : {
          "type" : "string",
          "default" : "-1s",
          "description" : "Default: -1s"
        },
        "development" : {
          "type" : [ "boolean", "string" ],
          "default" : false,
          "description" : "Default: false"
        }
      }
    },
    "hk2" : {
      "type" : "object",
      "additionalProperties" : false,
      "properties" : {
        "threadPolicy" : {
          "type" : "string",
          "default" : "FULLY_THREADED",
          "description" : "Default: FULLY_THREADED"
        },
        "threadCount" : {
          "type" : [ "number", "string" ],
          "default" : 1,
          "description" : "Default: 1"
        },
        "runLevel" : {
          "type" : [ "number", "string" ],
          "default" : 20,
          "description" : "Default: 20"
        },
        "immediateScope" : {
          "type" : "object",
          "additionalProperties" : false,
          "properties" : {
            "enabled" : {
              "type" : [ "boolean", "string" ],
              "default" : true,
              "description" : "Default: true"
            },
            "state" : {
              "type" : "string",
              "default" : "RUNNING",
              "description" : "Default: RUNNING"
            }
          }
        },
        "threadScope" : {
          "type" : "object",
          "additionalProperties" : false,
          "properties" : {
            "enabled" : {
              "type" : [ "boolean", "string" ],
              "default" : true,
              "description" : "Default: true"
            }
          }
        },
        "names" : {
          "type" : [ "array", "string" ]
        }
      }
    },
    "DefaultRunLevelContext" : {
      "type" : "object",
      "additionalProperties" : false,
      "properties" : {
        "enabled" : {
          "type" : [ "boolean", "string" ],
          "default" : true,
          "description" : "Default: true"
        }
      }
    },
    "configuration" : {
      "type" : "object",
      "additionalProperties" : false,
      "properties" : {
        "validation" : {
          "type" : "object",
          "additionalProperties" : false,
          "properties" : {
            "enabled" : {
              "type" : [ "boolean", "string" ],
              "default" : "{{ defaults.enabled }}",
              "description" : "Default: {{ defaults.enabled }} (true)"
            }
          }
        }
      }
    },
    "log4j2" : {
      "type" : "object",
      "additionalProperties" : false,
      "properties" : {
        "Configuration" : {
          "type" : "object",
          "additionalProperties" : false,
          "properties" : {
            "name" : {
              "type" : "string",
              "default" : "Xorcery",
              "description" : "Default: Xorcery"
            },
            "status" : {
              "type" : "string",
              "default" : "warn",
              "description" : "Default: warn"
            },
            "properties" : {
              "type" : "object",
              "additionalProperties" : true,
              "properties" : { }
            },
            "thresholdFilter" : {
              "type" : "object",
              "additionalProperties" : false,
              "properties" : {
                "level" : {
                  "type" : "string",
                  "default" : "info",
                  "description" : "Default: info"
                }
              }
            },
            "appenders" : {
              "type" : "object",
              "additionalProperties" : true,
              "properties" : { }
            },
            "Loggers" : {
              "type" : "object",
              "additionalProperties" : false,
              "properties" : {
                "logger" : {
                  "type" : [ "array", "string" ]
                },
                "Root" : {
                  "type" : "object",
                  "additionalProperties" : false,
                  "properties" : {
                    "level" : {
                      "type" : "string",
                      "default" : "info",
                      "description" : "Default: info"
                    },
                    "AppenderRef" : {
                      "type" : [ "array", "string" ]
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "opentelemetry" : {
      "type" : "object",
      "additionalProperties" : false,
      "properties" : {
        "global" : {
          "type" : "object",
          "additionalProperties" : false,
          "properties" : {
            "enabled" : {
              "type" : [ "boolean", "string" ],
              "default" : "{{ opentelemetry.enabled ? false | true }}",
              "description" : "Default: {{ opentelemetry.enabled ? false | true }} (true)"
            }
          }
        },
        "enabled" : {
          "type" : [ "boolean", "string" ],
          "default" : "{{ defaults.enabled }}",
          "description" : "Default: {{ defaults.enabled }} (true)"
        },
        "install" : {
          "type" : [ "boolean", "string" ],
          "default" : true,
          "description" : "Default: true"
        },
        "resource" : {
          "type" : "object",
          "additionalProperties" : false,
          "properties" : {
            "service.namespace" : {
              "type" : "string",
              "default" : "{{ instance.environment }}",
              "description" : "Default: {{ instance.environment }} (development)"
            },
            "service.name" : {
              "type" : "string",
              "default" : "{{ application.name }}",
              "description" : "Default: {{ application.name }} (application)"
            },
            "service.version" : {
              "type" : "string",
              "default" : "{{ application.version }}",
              "description" : "Default: {{ application.version }} (unknown)"
            },
            "service.instance.id" : {
              "type" : "string",
              "default" : "{{ instance.id }}",
              "description" : "Default: {{ instance.id }} (service.application)"
            },
            "host.name" : {
              "type" : "string",
              "default" : "{{ instance.fqdn }}",
              "description" : "Default: {{ instance.fqdn }} (service.xorcery.test)"
            },
            "host.ip" : {
              "type" : [ "array", "string" ]
            },
            "process.runtime.name" : {
              "type" : "string",
              "default" : "{{ SYSTEM.java_runtime_name }}",
              "description" : "Default: {{ SYSTEM.java_runtime_name }} (OpenJDK Runtime Environment)"
            },
            "process.runtime.version" : {
              "type" : "string",
              "default" : "{{ SYSTEM.java_runtime_version }}",
              "description" : "Default: {{ SYSTEM.java_runtime_version }} (17.0.8+7-LTS)"
            },
            "process.runtime.description" : {
              "type" : "string",
              "default" : "{{ SYSTEM.java_vm_vendor }} {{ SYSTEM.java_vm_name }} {{ SYSTEM.java_vm_version }}",
              "description" : "Default: {{ SYSTEM.java_vm_vendor }} {{ SYSTEM.java_vm_name }} {{ SYSTEM.java_vm_version }} (Amazon.com Inc. OpenJDK 64-Bit Server VM 17.0.8+7-LTS)"
            }
          }
        },
        "exporters" : {
          "type" : "object",
          "additionalProperties" : false,
          "properties" : {
            "logging" : {
              "type" : "object",
              "additionalProperties" : false,
              "properties" : {
                "enabled" : {
                  "type" : [ "boolean", "string" ],
                  "default" : "{{ opentelemetry.exporters.otlp.endpoint ? false | true }}",
                  "description" : "Default: {{ opentelemetry.exporters.otlp.endpoint ? false | true }} (true)"
                },
                "interval" : {
                  "type" : "string",
                  "default" : "10s",
                  "description" : "Default: 10s"
                }
              }
            },
            "jmx" : {
              "type" : "object",
              "additionalProperties" : false,
              "properties" : {
                "enabled" : {
                  "type" : [ "boolean", "string" ],
                  "default" : "{{ opentelemetry.exporters.otlp.endpoint ? false | defaults.development }}",
                  "description" : "Default: {{ opentelemetry.exporters.otlp.endpoint ? false | defaults.development }} (false)"
                },
                "interval" : {
                  "type" : "string",
                  "default" : "5s",
                  "description" : "Default: 5s"
                }
              }
            },
            "otlp" : {
              "type" : "object",
              "additionalProperties" : false,
              "properties" : {
                "endpoint" : {
                  "default" : null,
                  "description" : "Default: null"
                },
                "http" : {
                  "type" : "object",
                  "additionalProperties" : false,
                  "properties" : {
                    "enabled" : {
                      "type" : [ "boolean", "string" ],
                      "default" : "{{ opentelemetry.exporters.otlp.endpoint ? true | false }}",
                      "description" : "Default: {{ opentelemetry.exporters.otlp.endpoint ? true | false }} (false)"
                    },
                    "port" : {
                      "type" : [ "number", "string" ],
                      "default" : 4318,
                      "description" : "Default: 4318"
                    },
                    "compression" : {
                      "type" : "string",
                      "default" : "gzip",
                      "description" : "Default: gzip"
                    },
                    "headers" : {
                      "type" : "object",
                      "additionalProperties" : true,
                      "properties" : { }
                    },
                    "connectTimeout" : {
                      "type" : "string",
                      "default" : "{{ defaults.connectTimeout }}",
                      "description" : "Default: {{ defaults.connectTimeout }} (5s)"
                    },
                    "timeout" : {
                      "type" : "string",
                      "default" : "10s",
                      "description" : "Default: 10s"
                    },
                    "interval" : {
                      "type" : "string",
                      "default" : "10s",
                      "description" : "Default: 10s"
                    },
                    "aggregationTemporality" : {
                      "type" : "string",
                      "default" : "alwaysCumulative",
                      "description" : "Default: alwaysCumulative"
                    },
                    "scheduleDelay" : {
                      "type" : "string",
                      "default" : "5s",
                      "description" : "Default: 5s"
                    },
                    "exporterTimeout" : {
                      "type" : "string",
                      "default" : "60s",
                      "description" : "Default: 60s"
                    },
                    "maxExportBatchSize" : {
                      "type" : [ "number", "string" ],
                      "default" : 1000,
                      "description" : "Default: 1000"
                    },
                    "maxQueueSize" : {
                      "type" : [ "number", "string" ],
                      "default" : 10000,
                      "description" : "Default: 10000"
                    },
                    "logsEndpoint" : {
                      "type" : "string",
                      "default" : "{{ opentelemetry.exporters.otlp.endpoint }}:{{ opentelemetry.exporters.otlp.http.port }}/v1/logs",
                      "description" : "Default: {{ opentelemetry.exporters.otlp.endpoint }}:{{ opentelemetry.exporters.otlp.http.port }}/v1/logs (null:4318/v1/logs)"
                    },
                    "tracesEndpoint" : {
                      "type" : "string",
                      "default" : "{{ opentelemetry.exporters.otlp.endpoint }}:{{ opentelemetry.exporters.otlp.http.port }}/v1/traces",
                      "description" : "Default: {{ opentelemetry.exporters.otlp.endpoint }}:{{ opentelemetry.exporters.otlp.http.port }}/v1/traces (null:4318/v1/traces)"
                    },
                    "metricsEndpoint" : {
                      "type" : "string",
                      "default" : "{{ opentelemetry.exporters.otlp.endpoint }}:{{ opentelemetry.exporters.otlp.http.port }}/v1/metrics",
                      "description" : "Default: {{ opentelemetry.exporters.otlp.endpoint }}:{{ opentelemetry.exporters.otlp.http.port }}/v1/metrics (null:4318/v1/metrics)"
                    }
                  }
                }
              }
            }
          }
        },
        "sampler" : {
          "type" : "object",
          "additionalProperties" : false,
          "properties" : {
            "enabled" : {
              "type" : [ "boolean", "string" ],
              "default" : "{{ opentelemetry.enabled }}",
              "description" : "Default: {{ opentelemetry.enabled }} (true)"
            },
            "excludes" : {
              "type" : [ "array", "string" ]
            },
            "includes" : {
              "type" : [ "array", "string" ]
            }
          }
        },
        "meters" : {
          "type" : "object",
          "additionalProperties" : false,
          "properties" : {
            "excludes" : {
              "type" : [ "array", "string" ]
            },
            "includes" : {
              "type" : [ "array", "string" ]
            }
          }
        },
        "instrumentations" : {
          "type" : "object",
          "additionalProperties" : true,
          "properties" : { }
        }
      }
    },
    "secrets" : {
      "type" : "object",
      "additionalProperties" : false,
      "properties" : {
        "enabled" : {
          "type" : [ "boolean", "string" ],
          "default" : "{{defaults.enabled}}",
          "description" : "Default: {{defaults.enabled}} (true)"
        },
        "default" : {
          "type" : "string",
          "default" : "secret",
          "description" : "Default: secret"
        },
        "env" : {
          "type" : "object",
          "additionalProperties" : false,
          "properties" : {
            "enabled" : {
              "type" : [ "boolean", "string" ],
              "default" : "{{ secrets.enabled }}",
              "description" : "Default: {{ secrets.enabled }} ({{ secrets.enabled }})"
            }
          }
        },
        "system" : {
          "type" : "object",
          "additionalProperties" : false,
          "properties" : {
            "enabled" : {
              "type" : [ "boolean", "string" ],
              "default" : "{{ secrets.enabled }}",
              "description" : "Default: {{ secrets.enabled }} ({{ secrets.enabled }})"
            }
          }
        }
      }
    }
  }
}