{
  "$schema" : "https://json-schema.org/draft/2020-12/schema",
  "type" : "object",
  "$id" : "http://xorcery.exoreaction.com/modules/com.exoreaction.xorcery/xorcery-admin-server/schema",
  "title" : "xorcery-admin-server configuration JSON Schema",
  "properties" : {
    "$schema" : {
      "type" : "string",
      "default" : "./xorcery-schema.json",
      "description" : "Default: ./xorcery-schema.json"
    },
    "adminservlet" : {
      "$ref" : "#/$defs/adminservlet"
    },
    "serveradmin" : {
      "$ref" : "#/$defs/serveradmin"
    }
  },
  "$defs" : {
    "adminservlet" : {
      "type" : "object",
      "additionalProperties" : false,
      "properties" : {
        "enabled" : {
          "type" : [ "boolean", "string" ],
          "default" : "{{ defaults.enabled }}",
          "description" : "Default: {{ defaults.enabled }} (true)"
        }
      }
    },
    "serveradmin" : {
      "type" : "object",
      "additionalProperties" : false,
      "properties" : {
        "enabled" : {
          "type" : [ "boolean", "string" ],
          "default" : "{{ defaults.enabled }}",
          "description" : "Default: {{ defaults.enabled }} (true)"
        }
      }
    }
  }
}