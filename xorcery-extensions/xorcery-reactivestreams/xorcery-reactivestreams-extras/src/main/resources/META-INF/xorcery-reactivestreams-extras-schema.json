{
  "$schema" : "https://json-schema.org/draft/2020-12/schema",
  "type" : "object",
  "$id" : "http://xorcery.exoreaction.com/modules/dev.xorcery/xorcery-reactivestreams-extras/schema",
  "title" : "xorcery-reactivestreams-extras configuration JSON Schema",
  "properties" : {
    "yamlfilepublisher" : {
      "$ref" : "#/$defs/yamlfilepublisher"
    }
  },
  "$defs" : {
    "yamlfilepublisher" : {
      "type" : "object",
      "additionalProperties" : false,
      "properties" : {
        "enabled" : {
          "type" : [ "boolean", "string" ],
          "default" : "{{ defaults.enabled }}",
          "description" : "Default: {{ defaults.enabled }} (true)"
        },
        "publishers" : {
          "type" : [ "array", "string" ]
        }
      }
    }
  }
}