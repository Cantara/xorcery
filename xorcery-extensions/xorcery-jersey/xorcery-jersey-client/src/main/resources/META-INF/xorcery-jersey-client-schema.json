{
  "$schema" : "https://json-schema.org/draft/2020-12/schema",
  "type" : "object",
  "$id" : "http://xorcery.exoreaction.com/modules/com.exoreaction.xorcery/xorcery-jersey-client/schema",
  "title" : "xorcery-jersey-client configuration JSON Schema",
  "properties" : {
    "jersey" : {
      "$ref" : "#/$defs/jersey"
    }
  },
  "$defs" : {
    "jersey" : {
      "type" : "object",
      "additionalProperties" : false,
      "properties" : {
        "client" : {
          "type" : "object",
          "additionalProperties" : false,
          "properties" : {
            "enabled" : {
              "type" : [ "boolean", "string" ],
              "default" : "{{ jetty.client.enabled }}",
              "description" : "Default: {{ jetty.client.enabled }} (true)"
            },
            "keystore" : {
              "default" : null,
              "description" : "Default: null",
              "type" : [ "string", "null" ]
            },
            "truststore" : {
              "default" : null,
              "description" : "Default: null",
              "type" : [ "string", "null" ]
            },
            "connectTimeout" : {
              "type" : "string",
              "default" : "{{ defaults.connectTimeout }}",
              "description" : "Default: {{ defaults.connectTimeout }} (5s)"
            },
            "readTimeout" : {
              "type" : "string",
              "default" : "0s",
              "description" : "Default: 0s"
            },
            "properties" : {
              "type" : "object",
              "additionalProperties" : true,
              "properties" : { }
            }
          }
        }
      }
    }
  }
}