{
  "$schema" : "https://json-schema.org/draft/2020-12/schema",
  "type" : "object",
  "$id" : "http://xorcery.exoreaction.com/modules/com.exoreaction.xorcery/xorcery-dns-registration/schema",
  "title" : "xorcery-dns-registration configuration JSON Schema",
  "properties" : {
    "dns" : {
      "$ref" : "#/$defs/dns"
    }
  },
  "$defs" : {
    "dns" : {
      "type" : "object",
      "properties" : {
        "registration" : {
          "type" : "object",
          "properties" : {
            "enabled" : {
              "type" : "string",
              "default" : "{{ defaults.enabled }}",
              "description" : "Default: {{ defaults.enabled }} ({{ defaults.enabled }})"
            }
          }
        }
      }
    }
  }
}